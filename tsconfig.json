{
  "include": ["./**/*.ts" ],
  "exclude": ["dist", "node_modules", "**/*.spec.ts"],
  "compilerOptions": {
    "target": "ES6", // ou "ES2015"
    "module": "commonjs",
    "strict": true,
    "resolveJsonModule": true,
    "esModuleInterop": true,
    "skipLibCheck": true,
    "outDir": "./dist"
  }
}
